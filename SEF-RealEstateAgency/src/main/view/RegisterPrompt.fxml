<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="183.0" prefWidth="344.0" stylesheets="@../res/login.css" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.RegisterPromptController">
    <children>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#0c2940f2" height="245.0" layoutX="-1.0" stroke="BLACK"
                   strokeType="INSIDE" strokeWidth="0.0" width="458.0"/>
        <Button fx:id="propertyOwnerBtn" layoutX="67.0" layoutY="141.0" mnemonicParsing="false"
                onAction="#goToPropertyOwner" prefHeight="39.0" prefWidth="322.0"
                style="-fx-background-color: white; -fx-background-radius: 5;"
                text="I'm interested in selling or renting out">
            <font>
                <Font size="16.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button fx:id="customerBtn" layoutX="67.0" layoutY="65.0" mnemonicParsing="false" onAction="#goToCustomer"
                prefHeight="39.0" prefWidth="322.0" style="-fx-background-color: white; -fx-background-radius: 5;"
                text="I'm interested in buying or renting">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <font>
                <Font size="16.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
